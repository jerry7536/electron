{
  "name": "electron-vite-multi-asar",
  "version": "0.1.14",
  "main": "app.js",
  "description": "Electron example of multi asar",
  "author": "jerry7536",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "vue"
  ],
  "repository": "https://github.com/jerry7536/electron",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "node scripts/before-build.js && vite",
    "version:update": "vue-tsc --noEmit && node scripts/before-build.js && vite build && node scripts/before-pack.js",
    "build": "pnpm run version:update && electron-builder --config electron-builder.config.js",
    "preview": "vite preview",
    "test": "pnpm build && cd release/win-unpacked && .\\electron-vite-multi-asar.exe",
    "postinstall": "electron-builder install-app-deps",
    "version:release": "pnpm version:update",
    "release": "bumpp -x \"pnpm run version:release\" --all"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.1.9",
    "@types/better-sqlite3": "^7.6.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "ci-info": "^3.8.0",
    "electron": "^24.4.1",
    "electron-builder": "^23.6.0",
    "electron-settings": "^4.0.2",
    "eslint": "^8.41.0",
    "ofetch": "^1.0.1",
    "typescript": "^5.1.3",
    "unplugin-auto-expose": "^0.0.5",
    "vite": "^4.3.9",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  },
  "dependencies": {
    "better-sqlite3": "^8.4.0"
  }
}
